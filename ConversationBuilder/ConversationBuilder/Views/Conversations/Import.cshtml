@model ConversationBuilder.ViewModels.ImportViewModel

@{
	ViewData["Title"] = "Import";
	Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Styles {
	<link rel="stylesheet" href="~/css/@ViewBag.CssFile" />	
}


<h4>Import configuration to create conversations</h4>
<div>
	<form asp-action="Import">
		<div asp-validation-summary="ModelOnly" class="text-danger"></div>

		<div class="form-group">
			<div class="form-group">
			Config
			<textarea asp-for="Config" class="form-control"> </textarea>
			<span asp-validation-for="Config" class="text-danger"></span>
		</div>
		<div class="form-group">
			<input type="submit" value="Create" class="btn btn-default" />
		</div>
	</form>
</div>
